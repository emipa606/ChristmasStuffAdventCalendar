<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!--Items-->
  <ThingDef ParentName="OrganicProductBase">
    <defName>CSSuet</defName>
    <label>Suet</label>
    <description>Suet is the raw, hard fat of beef or mutton found around the loins and kidneys.</description>
    <graphicData>
      <texPath>Things/17th/Suet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>2.5</MarketValue>
      <Mass>0.03</Mass>
      <Nutrition>0.05</Nutrition>
      <FoodPoisonChanceFixedHuman>0.1</FoodPoisonChanceFixedHuman>
    </statBases>
    <socialPropernessMatters>true</socialPropernessMatters>
    <tickerType>Rare</tickerType>
    <ingestible>
      <foodType>AnimalProduct</foodType>
      <preferability>RawBad</preferability>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>RawVegetable_Eat</ingestSound>
    </ingestible>
    <thingCategories>
      <li>CSFoodCat</li>
    </thingCategories>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>7</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>
  <!--Christmas Pudding-->
  <ThingDef ParentName="MealCooked">
    <defName>CSCPudding</defName>
    <label>Christmas pudding</label>
    <description>Christmas pudding is a type of pudding traditionally served as part of the Christmas dinner.</description>
    <graphicData>
      <texPath>Things/17th/ChristmasPudding</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>18</MarketValue>
      <WorkToMake>450</WorkToMake>
      <Nutrition>0.6</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealFine</preferability>
      <tasteThought>CSAteCPudding</tasteThought>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
  </ThingDef>
  <!--AteThought-->
  <ThoughtDef>
    <defName>CSAteCPudding</defName>
    <durationDays>1</durationDays>
    <stages>
      <li>
        <label>ate christmas pudding</label>
        <description>That christmas pudding was amazing. It nourished my body and my soul.</description>
        <baseMoodEffect>3</baseMoodEffect>
      </li>
    </stages>
  </ThoughtDef>
  <!--Recipes-->
  <RecipeDef>
    <defName>CSMake_Suet</defName>
    <label>Extract suet</label>
    <description>Extract suet from cow.</description>
    <workerCounterClass>RecipeWorkerCounter_ButcherAnimals</workerCounterClass>
    <jobString>Butchering.</jobString>
    <workAmount>450</workAmount>
    <workSpeedStat>ButcheryFleshSpeed</workSpeedStat>
    <workSkill>Cooking</workSkill>
    <efficiencyStat>ButcheryFleshEfficiency</efficiencyStat>
    <effectWorking>ButcherFlesh</effectWorking>
    <soundWorking>Recipe_ButcherCorpseFlesh</soundWorking>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Corpse_Cow</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <specialProducts>
      <li>Butchery</li>
    </specialProducts>
    <products>
      <CSSuet>15</CSSuet>
    </products>
    <recipeUsers>
      <li>TableButcher</li>
    </recipeUsers>
  </RecipeDef>
  <RecipeDef>
    <defName>CSMake_CPudding</defName>
    <label>make christmas pudding</label>
    <description>Make christmas pudding.</description>
    <jobString>Preparing christmas dinner.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <workSkill>Cooking</workSkill>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>2000</workAmount>
    <targetCountAdjustment>1</targetCountAdjustment>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>CSFlour</li>
          </thingDefs>
        </filter>
        <count>25</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>CSRaisins</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>CSSugar</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>CSSuet</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>CSRawCinnamon</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>CSRum</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Milk</li>
          </thingDefs>
        </filter>
        <count>15</count>
      </li>
      <li>
        <filter>
          <categories>
            <li>EggsUnfertilized</li>
          </categories>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <products>
      <CSCPudding>10</CSCPudding>
    </products>
    <recipeUsers>
      <li>FueledStove</li>
      <li>ElectricStove</li>
    </recipeUsers>
  </RecipeDef>
</Defs>
